{
  "name": "mig-ui",
  "version": "0.1.0",
  "description": "Cluster Application Migration Tool",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean",
    "lint": "tslint --config tslint.json 'src/**/*.{ts,tsx}'",
