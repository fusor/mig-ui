{
  "name": "mig-ui",
  "version": "0.1.0",
  "description": "Cluster Application Migration Tool",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "webpack --mode=production --config config/webpack.prod.js",
    "image:build": "./scripts/build-image.sh",
    "image:push": "./scripts/push-image.sh",
    "clean": "yarn rimraf dist",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn eslint . --ext .js,.jsx,.ts,.tsx --fix ",
    "start": "DEVMODE=local webpack-dev-server --hot --color --progress --info=true --config config/webpack.dev.js",
    "start:prod-server": "./scripts/prod-server.sh",
    "start:remote": " DEPRECATED_CORS=false ./scripts/remote-dev.sh",
    "start:remote:3": "ORIGIN3_HOST=true ./scripts/remote-dev.sh",
    "prep-remote": "./scripts/prep-remote.sh",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "copy-config": "cp ./config/config.dev.json.example ./config/config.dev.json && ",
    "test:unit": "jest --rootDir=. --config=config/jest.config.js",
    "test:integration": "./scripts/test-integration.sh",
    "test": "yarn test:unit && yarn test:integration",
    "ci": "yarn lint && yarn test:unit && yarn build",
    "mocks": "node scripts/gather-mocked-data.js",
    "mocks:prune": "node scripts/gather-mocked-data.js prune"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.6",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "cypress": "^3.3.1",
    "eslint": "^6.8.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "json-merge-patch": "^0.2.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "redux-persist": "^5.10.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.3.1",
    "source-map-loader": "^0.2.4",
    "start-server-and-test": "^1.9.1",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.1",
    "ts-loader": "~5.3.3",
    "typed-scss-modules": "^1.1.0",
    "typescript": "^3.3.4000",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.9",
    "@patternfly/react-core": "^3.104.0",
    "@patternfly/react-table": "^2.20.15",
    "@rebass/emotion": "^3.1.0",
    "@types/react": "^16.8.17",
    "axios": "^0.19.0",
    "client-oauth2": "^4.2.3",
    "connected-react-router": "^6.3.2",
    "ejs": "^2.6.1",
    "emotion": "^10.0.9",
    "emotion-theming": "^10.0.9",
    "express": "^4.17.1",
    "formik": "^1.5.2",
    "history": "^4.9.0",
    "jszip": "^3.2.2",
    "lodash": "^4.17.14",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "q": "^1.5.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-json-view": "^1.19.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.4.2",
    "react-table": "^6.10.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.3",
    "redux-thunk": "^2.3.0",
    "reduxsauce": "^1.0.1",
    "reselect": "^4.0.0",
    "sass": "^1.22.10",
    "uuid": "^3.3.2"
  }
}
